{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:has_component",
    "component": "minecraft:enchantments",
    "ignore_default": true,
    "on_true": {
      "type": "minecraft:condition",
      "property": "minecraft:fishing_rod/cast",
      "on_false": {
        "type": "minecraft:select",
        "property": "minecraft:display_context",
        "cases": [
          {
            "when": [
              "fixed",
              "ground",
              "gui"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/fishing_rod"
            }
          },
          {
            "when": [
              "firstperson_lefthand",
              "firstperson_righthand"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/enchanted_fishing_rod_firstperson"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "minecraft:item/enchanted_fishing_rod"
        }
      },
      "on_true": {
        "type": "minecraft:select",
        "property": "minecraft:display_context",
        "cases": [
          {
            "when": [
              "fixed",
              "ground",
              "gui"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/fishing_rod_cast"
            }
          },
          {
            "when": [
              "firstperson_lefthand",
              "firstperson_righthand"
            ],
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/enchanted_fishing_rod_cast_firstperson"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "minecraft:item/enchanted_fishing_rod_cast"
        }
      }
    },
    "on_false": {
      "type": "minecraft:condition",
      "property": "minecraft:fishing_rod/cast",
      "on_false": {
        "type": "minecraft:model",
        "model": "minecraft:item/fishing_rod"
      },
      "on_true": {
        "type": "minecraft:model",
        "model": "minecraft:item/fishing_rod_cast"
      }
    }
  }
}