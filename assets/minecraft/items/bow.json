{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:has_component",
    "component": "minecraft:enchantments",
    "ignore_default": true,
    "on_true": {
      "type": "minecraft:select",
      "property": "minecraft:display_context",
      "cases": [
        {
          "when": [
            "gui",
            "ground",
            "fixed"
          ],
          "model": {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:use_duration",
            "scale": 0.05,
            "entries": [
              {
                "threshold": 0.1,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/bow_pulling_0"
                }
              },
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/bow_pulling_1"
                },
                "threshold": 0.65
              },
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/bow_pulling_2"
                },
                "threshold": 0.9
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/bow"
            }
          }
        },
        {
          "when": [
            "firstperson_lefthand",
            "firstperson_righthand"
          ],
          "model": {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:use_duration",
            "scale": 0.05,
            "entries": [
              {
                "threshold": 0.1,
                "model": {
                  "type": "minecraft:model",
                  "model": "item/enchanted_bow_pulling_0_firstperson"
                }
              },
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "item/enchanted_bow_pulling_1_firstperson"
                },
                "threshold": 0.65
              },
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "item/enchanted_bow_pulling_2_firstperson"
                },
                "threshold": 0.9
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "item/enchanted_bow_firstperson"
            }
          }
        }
      ],
      "fallback": {
        "type": "minecraft:range_dispatch",
        "property": "minecraft:use_duration",
        "scale": 0.05,
        "entries": [
          {
            "threshold": 0.1,
            "model": {
              "type": "minecraft:model",
              "model": "item/enchanted_bow_pulling_0"
            }
          },
          {
            "model": {
              "type": "minecraft:model",
              "model": "item/enchanted_bow_pulling_1"
            },
            "threshold": 0.65
          },
          {
            "model": {
              "type": "minecraft:model",
              "model": "item/enchanted_bow_pulling_2"
            },
            "threshold": 0.9
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "item/enchanted_bow"
        }
      }
    },
    "on_false": {
      "type": "minecraft:select",
      "property": "minecraft:display_context",
      "cases": [
        {
          "when": [
            "gui",
            "ground",
            "fixed"
          ],
          "model": {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:use_duration",
            "scale": 0.05,
            "entries": [
              {
                "threshold": 0.1,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/bow_pulling_0"
                }
              },
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/bow_pulling_1"
                },
                "threshold": 0.65
              },
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/bow_pulling_2"
                },
                "threshold": 0.9
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/bow"
            }
          }
        }
      ],
      "fallback": {
        "type": "minecraft:range_dispatch",
        "property": "minecraft:use_duration",
        "scale": 0.05,
        "entries": [
          {
            "threshold": 0.1,
            "model": {
              "type": "minecraft:model",
              "model": "item/bow_pulling_0"
            }
          },
          {
            "model": {
              "type": "minecraft:model",
              "model": "item/bow_pulling_1"
            },
            "threshold": 0.65
          },
          {
            "model": {
              "type": "minecraft:model",
              "model": "item/bow_pulling_2"
            },
            "threshold": 0.9
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "minecraft:item/bow"
        }
      }
    }
  }
}