{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:has_component",
    "component": "minecraft:enchantments",
    "ignore_default": true,
    "on_true": {
      "type": "minecraft:select",
      "cases": [
        {
          "model": {
            "type": "minecraft:composite",
            "models": [
              {
                "type": "minecraft:model",
                "model": "minecraft:item/trident_display"
              }
            ]
          },
          "when": [
            "gui",
            "ground",
            "fixed"
          ]
        },
        {
          "model": {
            "type": "minecraft:composite",
            "models": [
              {
                "type": "minecraft:special",
                "model": {
                  "type": "minecraft:trident"
                },
                "base": "minecraft:item/trident_in_hand"
              },
              {
                "type": "minecraft:model",
                "model": "minecraft:item/enchanted_trident_in_hand_firstperson"
              }
            ]
          },
          "when": [
            "firstperson_lefthand",
            "firstperson_righthand"
          ]
        }
      ],
      "fallback": {
        "type": "minecraft:condition",
        "on_true": {
          "type": "minecraft:select",
          "cases": [
            {
              "model": {
                "type": "minecraft:composite",
                "models": [
                  {
                    "type": "minecraft:special",
                    "model": {
                      "type": "minecraft:trident"
                    },
                    "base": "minecraft:item/trident_throwing"
                  },
                  {
                    "type": "minecraft:model",
                    "model": "minecraft:item/enchanted_trident_throwing_firstperson"
                  }
                ]
              },
              "when": [
                "firstperson_lefthand",
                "firstperson_righthand"
              ]
            }
          ],
          "fallback": {
            "type": "minecraft:composite",
            "models": [
              {
                "type": "minecraft:special",
                "model": {
                  "type": "minecraft:trident"
                },
                "base": "minecraft:item/trident_throwing"
              },
              {
                "type": "minecraft:model",
                "model": "minecraft:item/enchanted_trident_throwing"
              }
            ]
          },
          "property": "minecraft:display_context"
        },
        "on_false": {
          "type": "minecraft:select",
          "cases": [
            {
              "model": {
                "type": "minecraft:composite",
                "models": [
                  {
                    "type": "minecraft:special",
                    "model": {
                      "type": "minecraft:trident"
                    },
                    "base": "minecraft:item/trident_in_hand"
                  },
                  {
                    "type": "minecraft:model",
                    "model": "minecraft:item/enchanted_trident_in_hand_firstperson"
                  }
                ]
              },
              "when": [
                "firstperson_lefthand",
                "firstperson_righthand"
              ]
            }
          ],
          "fallback": {
            "type": "minecraft:composite",
            "models": [
              {
                "type": "minecraft:special",
                "model": {
                  "type": "minecraft:trident"
                },
                "base": "minecraft:item/trident_in_hand"
              },
              {
                "type": "minecraft:model",
                "model": "minecraft:item/enchanted_trident_in_hand"
              }
            ]
          },
          "property": "minecraft:display_context"
        },
        "property": "minecraft:using_item"
      },
      "property": "minecraft:display_context"
    },
    "on_false": {
      "type": "minecraft:condition",
      "property": "minecraft:has_component",
      "component": "minecraft:enchantments",
      "ignore_default": true,
      "on_true": {
        "type": "minecraft:select",
        "cases": [
          {
            "model": {
              "type": "minecraft:composite",
              "models": [
                {
                  "type": "minecraft:model",
                  "model": "minecraft:item/trident_display"
                }
              ]
            },
            "when": [
              "gui",
              "ground",
              "fixed"
            ]
          }
        ],
        "fallback": {
          "type": "minecraft:condition",
          "on_true": {
            "type": "minecraft:composite",
            "models": [
              {
                "type": "minecraft:special",
                "model": {
                  "type": "minecraft:trident"
                },
                "base": "minecraft:item/trident_throwing"
              },
              {
                "type": "minecraft:model",
                "model": "minecraft:item/enchanted_trident_throwing"
              }
            ]
          },
          "on_false": {
            "type": "minecraft:composite",
            "models": [
              {
                "type": "minecraft:special",
                "model": {
                  "type": "minecraft:trident"
                },
                "base": "minecraft:item/trident_in_hand"
              },
              {
                "type": "minecraft:model",
                "model": "minecraft:item/enchanted_trident_in_hand"
              }
            ]
          },
          "property": "minecraft:using_item"
        },
        "property": "minecraft:display_context"
      },
      "on_false": {
        "type": "minecraft:select",
        "cases": [
          {
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/trident_display"
            },
            "when": [
              "gui",
              "ground",
              "fixed"
            ]
          }
        ],
        "fallback": {
          "type": "minecraft:condition",
          "on_false": {
            "type": "minecraft:special",
            "base": "minecraft:item/trident_in_hand",
            "model": {
              "type": "minecraft:trident"
            }
          },
          "on_true": {
            "type": "minecraft:special",
            "base": "minecraft:item/trident_throwing",
            "model": {
              "type": "minecraft:trident"
            }
          },
          "property": "minecraft:using_item"
        },
        "property": "minecraft:display_context"
      }
    }
  }
}